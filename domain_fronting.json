{
  "mappings": [
    {
      "patterns": [
        "upload.wikimedia.org"
      ],
      "server": "upload.wikimedia.org",
      "port": 443
    },
    {
      "patterns": [
        "*.wikipedia.org",
        "wikimedia.org",
        "*.wikimedia.org"
      ],
      "server": "wikivoyage.org",
      "port": 443
    },
    {
      "patterns": [
        "*.google.com",
        "*.gstatic.com",
        "*.googleapis.com",
        "*.googleusercontent.com",
        "*.ytimg.com",
        "*.youtube.com",
        "*.ggpht.com"
      ],
      "server": "www.gstatic.cn",
      "port": 443
    },
    {
      "patterns": [
        "github.com",
        "gist.github.com"
      ],
      "server": "octocaptcha.com",
      "port": 443
    },
    {
      "patterns": [
        "duckduckgo.com",
        "*.duckduckgo.com"
      ],
      "server": "duck.com",
      "port": 443
    },
    {
      "patterns": [
        "twitter.com",
        "*.twitter.com",
        "abs.twimg.com",
        "pbs.twimg.com"
      ],
      "server": "tweetdeck.com",
      "port": 443
    },
    {
      "patterns": [
        "abs-0.twimg.com"
      ],
      "server": "twimg.com",
      "port": 443
    },
    {
      "patterns": [
        "video.twimg.com"
      ],
      "server": "test.twimg.com",
      "port": 443
    }
  ]
}

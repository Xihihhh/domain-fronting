{
  "mappings": [
    {
      "patterns": [
        "upload.wikimedia.org"
      ],
      "server": "upload.wikimedia.org",
      "port": 443
    },
    {
      "patterns": [
        "*.wikipedia.org",
        "wikimedia.org",
        "*.wikimedia.org"
      ],
      "server": "wikivoyage.org",
      "port": 443
    },
    {
      "patterns": [
        "google.com",
        "youtube.com",
        "*.google.com",
        "*.gstatic.com",
        "*.googleapis.com",
        "*.googleusercontent.com",
        "*.ytimg.com",
        "*.youtube.com",
        "*.ggpht.com"
      ],
      "server": "www.gstatic.cn",
      "port": 443
    },
    {
      "patterns": [
        "github.com",
        "gist.github.com"
      ],
      "server": "octocaptcha.com",
      "port": 443
    },
    {
      "patterns": [
        "duckduckgo.com",
        "*.duckduckgo.com"
      ],
      "server": "duck.com",
      "port": 443
    },
    {
      "patterns": [
        "twitter.com",
        "*.twitter.com",
        "abs.twimg.com",
        "pbs.twimg.com"
      ],
      "server": "tweetdeck.com",
      "port": 443
    },
    {
      "patterns": [
        "video.twimg.com"
      ],
      "server": "test.twimg.com",
      "port": 443
    },
    {
      "patterns": [
        "pixiv.net",
        "*.pixiv.net"
      ],
      "server": "fanbox.cc",
      "port": 443
    },
    {
      "patterns": [
        "*.pximg.net"
      ],
      "server": "pximg.net",
      "port": 443
    },
    {
      "patterns": [
        "quora.com",
        "*.quora.com"
      ],
      "server": "qr.ae",
      "port": 443
    }
  ]
}
